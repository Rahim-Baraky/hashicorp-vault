<% @properties.each do |key, value| -%>
<% if value.is_a?(Hash) -%>
<%= key.to_s %> = {
<%= template_partial_indent(render('vault/_hcl_settings.erb', cookbook: 'hashicorp-vault', variables: { name: key.to_s, properties: value }), 1) %>
}
<% else -%>
<%= key %> = <%= vault_hcl_value(value) %>
<% end -%>
<% end -%>
